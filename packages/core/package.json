{
  "name": "@glyph-cat/swiss-army-knife",
  "version": "0.0.0-experimental.41",
  "main": "lib/cjs/index.js",
  "module": "lib/es/index.js",
  "react-native": "lib/native/index.js",
  "unpkg": "lib/umd/index.js",
  "types": "lib/types/index.d.ts",
  "files": [
    "assets",
    "lib"
  ],
  "author": "glyph-cat",
  "license": "MIT",
  "readme": "README.md",
  "keywords": [
    "react",
    "react-native",
    "common utilities",
    "javascript",
    "typescript"
