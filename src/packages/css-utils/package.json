{
  "name": "@glyph-cat/css-utils",
  "version": "1.0.0",
  "description": "CSS-related utilities.",
  "main": "lib/cjs/index.js",
  "module": "lib/es/index.js",
  "unpkg": "lib/umd/index.js",
  "files": [
    "lib"
  ],
  "types": "lib/types/index.d.ts",
  "scripts": {
    "afe": "tsx ./scripts/run/auto-forward-exports",
    "clean": "rm -rf ./lib ./temp/tsc",
    "lint": "eslint .",
    "debug": "scope=\"debug\" jest -c ./jest.config.ts",
    "bundle": "rollup -c ./config/rollup.config.js",
    "types": "yarn tsc --declaration",
    "api": "api-extractor run -c ./config/api-extractor.json --local --verbose",
    "build": "sh ./scripts/run/build.sh",
    "test": "jest -c ./jest.config.ts",
    "bump": "yarn --cwd ../../../ bump css-utils",
    "all": "sh ./scripts/run/all.sh"
  },
  "keywords": [
    "CSS"
  ],
  "author": "glyph-cat",
  "readme": "README.md",
  "license": "MIT",
  "homepage": "https://github.com/glyph-cat/swiss-army-knife/tree/main/src/packages/css-utils#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/glyph-cat/swiss-army-knife.git"
  },
  "bugs": {
    "url": "https://github.com/glyph-cat/swiss-army-knife/issues"
  },
  "dependencies": {
    "@glyph-cat/cleanup-manager": "^1.0.0",
    "@glyph-cat/foundation": "0.0.0-experimental.88",
    "@glyph-cat/type-checking": "^1.0.0",
    "autoprefixer": "^10.4.27",
    "postcss": "^8.5.3"
  }
}