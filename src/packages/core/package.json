  "keywords": [
    "common utilities",
    "javascript",
    "typescript"
  ],
  "homepage": "https://github.com/glyph-cat/swiss-army-knife#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/glyph-cat/swiss-army-knife.git"
  },
  "bugs": {
    "url": "https://github.com/glyph-cat/swiss-army-knife/issues"
  },
  "dependencies": {
    "@glyph-cat/css-utils": "^1.0.0",
    "@glyph-cat/equality": "^1.2.0",
    "@glyph-cat/foundation": "0.0.0-experimental.88",
    "@glyph-cat/type-checking": "^1.0.0",
    "autoprefixer": "^10.4.21",
