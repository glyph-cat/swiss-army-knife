{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "mainEntryPointFilePath": "<projectFolder>/temp/tsc/types/react/src/index.d.ts",
  "compiler": {
    "overrideTsconfig": {
      "compilerOptions": {
        "paths": {
          "@glyph-cat/swiss-army-knife": [
            "./temp/types/core/src"
          ]
        }
      }
    }
  },
  "apiReport": {
    "enabled": false
  },
  "docModel": {
    "enabled": false
  },
  "dtsRollup": {
    "enabled": true,
    // `untrimmedFilePath` defaults to 'dist' folder, must use empty string to
    // disable it
    "untrimmedFilePath": "",
    "publicTrimmedFilePath": "<projectFolder>/lib/types/index.d.ts",
    "omitTrimmingComments": true
  },
  "tsdocMetadata": {
    "enabled": false
  },
  "messages": {
    "compilerMessageReporting": {
      "default": {
        "logLevel": "none"
      }
    },
    "extractorMessageReporting": {
      "default": {
        "logLevel": "warning"
      }
    },
    "tsdocMessageReporting": {
      "default": {
        "logLevel": "warning"
      },
      "tsdoc-escape-right-brace": {
        "logLevel": "none"
      },
      "tsdoc-escape-greater-than": {
        "logLevel": "none"
      },
      "tsdoc-html-tag-missing-equals": {
        "logLevel": "none"
      },
      "tsdoc-html-tag-missing-greater-than": {
        "logLevel": "none"
      },
      "tsdoc-html-tag-missing-string": {
        "logLevel": "none"
      },
      "tsdoc-malformed-html-name": {
        "logLevel": "none"
      },
      "tsdoc-malformed-inline-tag": {
        "logLevel": "none"
      },
      "tsdoc-param-tag-with-invalid-type": {
        "logLevel": "none"
      },
      "tsdoc-undefined-tag": {
        "logLevel": "none"
      },
      "tsdoc-unnecessary-backslash": {
        "logLevel": "none"
      }
    }
  }
}